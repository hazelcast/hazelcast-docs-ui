<script>
  window.UTILS = {
    parseHbsJsonString(str) {
      return JSON.parse(
        str
        .replace(/&quot;/g, '"')
        .replaceAll(String.fromCharCode(92), `${String.fromCharCode(92)}${String.fromCharCode(92)}`)
      );
    }
  }
</script>

