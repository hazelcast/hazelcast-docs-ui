<article class="doc">
{{#if (eq page.layout '404')}}
  <h1 class="page" data-id="article-header">{{{or page.title 'Page Not Found'}}}</h1>
  <div class="paragraph">
    <p>The page you&#8217;re looking for does not exist. It may have been moved.</p>
  </div>
  <div class="paragraph">
    <p>If you arrived on this page by clicking on a link, please notify the owner of the site that the link is broken.
  If you entered the URL of this page manually, please double check that you entered the address correctly.</p>
  </div>
  <div class=" paragraph help">
    <h2><img src="{{{uiRootPath}}}/img/search-404.svg">Find what you're looking for</h2>
  </div>
  <div class="paragraph find">
    <div class="find-item">
      <p>Useful Links</p>
      <ul>
        <li><a href="https://docs.hazelcast.com/hazelcast/latest/getting-started/get-started-cli">Quickstart</a></li>
        <li><a href="https://docs.hazelcast.com/hazelcast/latest/getting-started/install-hazelcast">Installing Hazelcast Community Edition</a></li>
        <li><a href="https://docs.hazelcast.com/hazelcast/latest/configuration/understanding-configuration">Hazelcast Configuration</a></li>
      </ul>
    </div>
  </div>
  {{> nav-toggle}}
  {{> page-versions}}
{{else}}
  {{> breadcrumbs}}
  {{#if (not-eq page.layout 'tutorial')}}
    {{>nav-toggle}}
  {{/if}}
  {{#if (not-eq page.layout 'swagger')}}
    {{#with page.title}}
      <h1
        data-id="article-header"
        class="page
      {{~#if (eq @root.page.attributes.enterprise 'true')}} enterprise{{/if}}
      {{~#if (eq @root.page.attributes.beta 'true')}} beta{{/if}}
      {{~#if (eq @root.page.attributes.dedicated 'true')}} dedicated{{/if}}
      {{~#if (eq @root.page.attributes.serverless 'true')}} serverless{{/if}}">{{{this}}}</h1>
    {{/with}}
  {{/if}}
  {{#if page.attributes.api-reference}}
    <div style="padding-top: 5px;">
      <a href="{{{page.attributes.api-reference}}}" onclick="window.open(this.href,'_blank');return false;">
      API docs
      </a>
    </div>
  {{/if}}
  {{> page-versions}}
  <div data-id="article-content">
    {{{page.contents}}}
  </div>
  {{#if page.attributes.include-tutorial-list}}
    <ul style="padding-top:20px;">
    {{#each (get-tutorial-pages 'cloud')}}
      <li><a href="{{{this.pub.url}}}">{{this.asciidoc.attributes.doctitle}}</a></li>
    {{/each}}
    </ul>
  {{/if}}
{{/if}}
{{> pagination}}
</article>
